<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="CENTER" hgap="10" prefHeight="300.0" prefWidth="429.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="namesayer.Controller">
    <padding><Insets bottom="10" left="25" right="25" top="25" /></padding>



    <HBox alignment="CENTER" GridPane.rowIndex="6" />

   <TreeView fx:id="treeView" prefHeight="144.0" prefWidth="159.0" GridPane.rowIndex="1" GridPane.rowSpan="5">
      <effect>
         <Blend />
      </effect>
   </TreeView>

   <CheckBox fx:id="handleShuffle" mnemonicParsing="false" prefHeight="18.0" prefWidth="76.0" text="Shuffle" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="3" />

    <ListView fx:id="practiceList" prefHeight="153.0" prefWidth="116.0" GridPane.columnIndex="2" GridPane.rowIndex="1" GridPane.rowSpan="2" />
     <!--<Button contentDisplay="CENTER" maxHeight="1.7976931348623157E308" onAction="#handleHomeButtonAction" prefWidth="160.0" text="Return to main menu" textAlignment="CENTER" />-->

    <Button onAction="#playAction" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" text="Playback" textAlignment="CENTER" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="4" />

    <Label text="Selected files" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
    <Label text="Practices" GridPane.columnIndex="2" GridPane.rowIndex="0" />
    <Label text="Database" GridPane.rowIndex="0" />

    <HBox alignment="CENTER" prefHeight="29.0" prefWidth="112.0" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="3">
      <children>
         <Button onAction="#newPrac" mnemonicParsing="false" text="+">
            <HBox.margin>
               <Insets right="5.0" />
            </HBox.margin></Button>
         <Button onAction="#delPrac" mnemonicParsing="false" text="-">
            <HBox.margin>
               <Insets left="5.0" />
            </HBox.margin></Button>
      </children>
      <GridPane.margin>
         <Insets top="5.0" />
      </GridPane.margin>
   </HBox>

   <ListView fx:id="playList" prefHeight="134.0" prefWidth="117.0" GridPane.columnIndex="3" GridPane.rowIndex="1" GridPane.rowSpan="2"  />







</GridPane>
